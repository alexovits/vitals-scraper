    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run \"crash\"",
            "type": "node",
            "request": "launch",
            "args": [
                "--nolazy",
                "-r",
                "ts-node/register",
                "-r",
                "tsconfig-paths/register",
                "${workspaceRoot}/src/index.ts",
                "--accountId=9116215767541857492",
                "--packageName=org.kiwix.kiwixmobile,org.kiwix.kiwixcustomphet",
                "--days=1",
                "--numExceptions=2",
                "--format=json",
                // "--parallel=4"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
        },
        {
            "name": "Run \"crash\" for all apps using wildcard",
            "type": "node",
            "request": "launch",
            "args": [
                "--nolazy",
                "-r",
                "ts-node/register",
                "-r",
                "tsconfig-paths/register",
                "${workspaceRoot}/src/index.ts",
                "--accountId=9116215767541857492",
                "--packageName=*",
                "--days=1",
                "--numExceptions=3",
                "--format=json",
                // "--parallel=4"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
        },
        {
            "name": "Run \"overview\"",
            "type": "node",
            "request": "launch",
            "args": [
                "--nolazy",
                "-r",
                "ts-node/register",
                "-r",
                "tsconfig-paths/register",
                "${workspaceRoot}/src/index.ts",
                "--accountId=9116215767541857492",
                "--mode=overview"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
        },
        {
            "name": "Run Playground",
            "type": "node",
            "request": "launch",
            "args": [
                "--nolazy",
                "-r",
                "ts-node/register",
                "${workspaceRoot}/src/data-playground.ts",
                "--data=./android-crash-clusters_1557226424411.json"
            ],
            "sourceMaps": true,
            "cwd": "${workspaceRoot}",
            "protocol": "inspector",
        }
    ]
}